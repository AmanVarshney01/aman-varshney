---
import { Image } from 'astro:assets'
import type { Project } from '../lib/types'

type Props = Project

const { name, loading, alias, githubLink } = Astro.props
---

<a href={`/projects/${alias}`} class="card block p-4">
	<div class="space-y-2">
		<div class="flex items-center justify-between">
			<h3 class="text-foreground">{name}</h3>
		</div>
	</div>
</a>
